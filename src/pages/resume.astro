        <div class="space-y-6">
          {
            organizations.map((org) => (
              <div class="bg-white rounded-2xl shadow-[0_1px_3px_rgba(0,0,0,0.04),_0_1px_2px_rgba(0,0,0,0.02)] border border-gray-200 transition-all duration-250 overflow-hidden p-6">
