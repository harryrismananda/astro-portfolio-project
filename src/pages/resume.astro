<Layout
  seo={{
    title: "Resume | Harry Rismananda",
    description:
      "Full Stack Web Developer with expertise in React, Node.js, and modern web technologies. View my professional resume and experience.",
    image: "/profile-picture.jpg",
    type: "website",
